@{
    ViewBag.Title = "Home Page";
}
@section featured {
    <section class="featured">
        <div class="content-wrapper">
            <hgroup class="title">
                <h1>@ViewBag.Title.</h1>
                <h2>@ViewBag.Message</h2>
            </hgroup>
        </div>
    </section>
}

@{
    var games = ViewBag.games;    
}

<ul>
@foreach (var g in games)
{
    //<li>@g.Name</li>
    
    <li>@Html.ActionLink((string) g.Name, "Index", "game", new {id = g.GameID}, null)</li>
}
</ul>